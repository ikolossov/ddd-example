{
  "name": "dream/skeleton",
  "type": "project",
  "description": "The skeleton application for DDD development with Laravel framework.",
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^11",
    "ramsey/uuid": "^4",
    "zircote/swagger-php": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "^11",
    "fakerphp/faker": "^1.23",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}